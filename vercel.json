{
  "rewrites": [
    {
      "source": "/gh/:match*",
      "destination": "https://github.com/:match*"
    },
    {
      "source": "/gh/raw/:match*",
      "destination": "https://raw.githubusercontent.com/:match*"
    },
    {
      "source": "/gh/gist/:match*",
      "destination": "https://gist.github.com/:match*"
    },
    {
      "source": "/gl/:match*",
      "destination": "https://gitlab.com/:match*"
    },
    {
      "source": "/bb/:match*",
      "destination": "https://bitbucket.com/:match*"
    },
    {
      "source": "/glitch/:match*",
      "destination": "https://api.glitch.com/git/:match*"
    }
  ],
  "cleanUrls": true
}
